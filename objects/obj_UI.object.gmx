        <arguments>
          <argument>
            <kind>1</kind>
            <string>//The specific object that the UI is written for
ui_object = obj_player.id;
//The variables used for drawing the bars (for prettier and smooth animations)
ui_maxspd = 0;
ui_maxspdStart = ui_object.maxspd;
ui_shootRate = 0;
ui_shootRateStart = ui_object.shootRate;
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="8" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (instance_exists(ui_object)){
    var half = obj_controller.map_realSizeW/2 //half the horisontal size of the room
    var barColor; //the color drawn on the bars
    
    //Drawing the UI frame
    draw_set_alpha(0.2);
    draw_set_color(c_black);
    draw_rectangle(half-500,0,half+500,250,0);
    
    //draw maxspeed
    draw_set_color(c_white);
    draw_set_alpha(1);
    draw_set_font(smallFont);
    draw_text(half-460,6,"Speed:");
        //changing values of max speed
    if (ui_object.maxspd-ui_maxspdStart &gt; ui_maxspd){
        ui_maxspd += 0.01;
        barColor = c_green;
    }
    else{
        barColor = c_blue;
    }
        //draw the bar itself
    draw_healthbar(half-300,10,
    half-100,40,
    (ui_maxspd / (ui_object.highestMaxSpeed-ui_maxspdStart))*100
    ,c_gray,barColor,barColor,0,1,1);
    barColor = c_blue;
        //draw the amount next to the bar
    draw_text(half-70,6,string(round(ui_maxspd*10)));
    
    
    //draw shootrate
    draw_set_color(c_white);
    draw_set_alpha(1);
    draw_set_font(smallFont);
    draw_text(half-460,38,"Shoot rate:");
    
        //changing values of shoot rate
    if (ui_object.shootRate-ui_shootRateStart &gt; ui_shootRate){
        ui_shootRate += 0.05;
        barColor = c_green;
    }
    else{
        barColor = c_blue;
    }
        //draw then bar itself
    draw_healthbar(half-300,42,
    half-100,72,
    (ui_shootRate / (ui_object.highestShootRate-ui_shootRateStart))*100
    ,c_gray,barColor,barColor,0,1,1);
    barColor = c_blue;
            //draw the amount next to the bar
    draw_text(half-70,38,string(round(ui_shootRate)));
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
  </events>
